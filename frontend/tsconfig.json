{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "jsx": "react-native",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true
    },
    "include": [
        "index.ts",
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}